{
  "docsets_to_publish": [
    {
      "docset_name": "powerapps-docs",
      "build_source_folder": "powerapps-docs",
      "build_output_subfolder": "powerapps-docs",
      "locale": "en-us",
      "monikers": [],
      "moniker_ranges": [],
      "xref_query_tags": [
        "/dotnet",
        "/power-apps/developer/data-platform/webapi/reference"
      ],
      "open_to_public_contributors": true,
      "type_mapping": {
        "Conceptual": "Content",
        "ManagedReference": "Content",
        "RestApi": "Content",
        "LandingData": "Content",
        "ContextObject": "Toc",
        "Tutorial": "Content"
      },
      "build_entry_point": "docs",
      "template_folder": "_themes",
      "moniker_groups": [],
      "version": 0,
      "customized_tasks": {
        "docset_prebuild": [
          "_dependentPackages/CommonPlugins/tools/JoinTOC.ps1"
        ]
      }
    }
  ],
  "notification_subscribers": [],
  "sync_notification_subscribers": [
    "buckley.guderian@microsoft.com"
  ],
  "branches_to_filter": [],
  "git_repository_url_open_to_public_contributors": "https://github.com/MicrosoftDocs/powerapps-docs",
  "git_repository_branch_open_to_public_contributors": "main",
  "skip_source_output_uploading": false,
  "need_preview_pull_request": true,
  "contribution_branch_mappings": {},
  "dependent_repositories": [
    {
      "path_to_root": "_themes",
      "url": "https://github.com/Microsoft/templates.docs.msft",
      "branch": "main",
      "branch_mapping": {}
    },
    {
      "path_to_root": "_themes.pdf",
      "url": "https://github.com/Microsoft/templates.docs.msft.pdf",
      "branch": "main",
      "branch_mapping": {}
    },
    {
      "path_to_root": "dynamics365-docs-dotnet",
      "url": "https://github.com/MicrosoftDocs/dynamics365-docs-dotnet",
      "branch": "live",
      "branch_mapping": {
        "master": "master",
        "live": "live"
      }
    },
    {
      "path_to_root": "PowerApps-Samples",
      "url": "https://github.com/microsoft/PowerApps-Samples",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "shared-content",
      "url": "https://github.com/MicrosoftDocs/powerapps-docs-pr",
      "branch": "main",
      "branch_mapping": {}
    }   
  ],
  "branch_target_mapping": {
    "live": [
      "Publish",
      "PDF"
    ],
    "main": [
      "Publish",
      "PDF"
    ],
    "alm-guide": [
      "Publish",
      "PDF"
    ]
  },
  "need_generate_pdf_url_template": true,
  "targets": {
    "Pdf": {
      "template_folder": "_themes.pdf"
    }
  },
  "docs_build_engine": {
    "name": "docfx_v3"
  },
  "dependent_packages": [
    {
      "id": "Microsoft.OpenPublishing.CommonPlugins",
      "nuget_feed": "https://www.myget.org/F/op/api/v2",
      "path_to_root": "_dependentPackages/CommonPlugins",
      "target_framework": "net45",
      "version": "latest"
    }
  ],
  "JoinTOCPlugin": [
    {
      "ConceptualTOC": "powerapps-docs/developer/data-platform/webapi/toc.yml",
      "ReferenceTOCUrl": "/power-apps/developer/data-platform/webapi/reference/toc.json"
    },
    {
      "ConceptualTOC": "powerapps-docs/developer/data-platform/org-service/toc.yml",
