{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/includes/media/**"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/*.md"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "contributors_to_exclude": ["openpublishingbuild","sudeepku"],
      "breadcrumb_path": "breadcrumb/toc.yml",
      "searchScope": ["PowerApps"] 
    },
    "fileMetadata": {},
    "template": [],
    "dest": "powerapps-docs",
    "titleSuffix": "PowerApps"
  }
}